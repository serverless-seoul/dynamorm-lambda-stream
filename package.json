{
  "name": "@serverless-seoul/dynamorm",
  "version": "1.0.0-beta",
  "description": "DynamoDB Stream Framework",
  "main": "./dst/index.js",
  "typings": "./dst/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/serverless-seoul/dynamorm-stream.git"
  },
  "keywords": [
    "DynamoDB",
    "AWS",
    "Typescript",
    "ORM",
    "Serverless"
  ],
  "bugs": {
    "url": "https://github.com/serverless-seoul/dynamorm-stream/issues"
  },
  "homepage": "https://github.com/serverless-seoul/dynamorm-stream",
  "scripts": {
    "clean": "rm -rf dst",
    "prebuild": "npm run clean",
    "build:copy": "cd src && find . -name '*.json' -type f -exec cp {} ../dst/{} \\; && cd ..",
    "build": "tsc && npm run build:copy",
    "pretest": "tsc -p tsconfig.test.json && npm run build:copy",
    "test": "AWS_REGION=us-east-1 AWS_ACCESS_KEY_ID=mock AWS_SECRET_ACCESS_KEY=mock mocha -t 20000 dst/**/__test__/**/*.js",
    "test:ci": "npm run test -- --forbid-only",
    "prepublishOnly": "npm run build",
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.ts'"
  },
  "author": "@serverless-seoul",
  "engines": {
    "node": "^8.10.0"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/debug": "4.1.5",
    "@types/faker": "4.1.10",
    "@types/mocha": "5.2.7",
    "@types/node": "8.10.59",
    "@vingle/tslint-preset": "1.0.1",
    "aws-sdk": "^2.300.0",
    "chai": "4.2.0",
    "debug": "4.1.1",
    "@serverless-seoul/dynamorm": "^1.0.0",
    "faker": "4.1.0",
    "mocha": "6.2.2",
    "tslint": "5.20.1",
    "typescript": "3.7.5"
  },
  "peerDependencies": {
    "@serverless-seoul/dynamorm": "^1.0.0",
    "aws-sdk": "^2.300.0",
    "@types/node": ">= 8.10.0"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.27"
  }
}
